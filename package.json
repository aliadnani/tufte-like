    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.12",
    "eleventy-plugin-reading-time": "^0.0.1",
    "html-minifier": "^4.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "start": "eleventy --serve",
    "fmt": "prettier --write --ignore-path .gitignore .",
    "build": "run-s clean build:assets build:site",
    "dev:assets": "env ELEVENTY_ENV=development webpack --mode production --watch",
    "dev:site": "env ELEVENTY_ENV=development eleventy --serve",
    "build:assets": "webpack --mode production",
    "build:site": "env ELEVENTY_ENV=production eleventy",
    "serve:site": "env ELEVENTY_ENV=production eleventy --serve",
    "clean": "rm -rf ./public"
  }
